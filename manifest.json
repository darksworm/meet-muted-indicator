{
	"name": "Google Meet Muted Indicator",
	"version": "0.0.1",
	"description": "Aggressively displays that you're muted in google meets",
	"homepage_url": "https://github.com/darksworm/meet-muted-indicator",
	"manifest_version": 2,
	"minimum_chrome_version": "74",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["muted-indicator.js"]
        }
    ]
}
